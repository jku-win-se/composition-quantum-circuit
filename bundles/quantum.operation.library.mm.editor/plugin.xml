         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated quantumOpe -->
      <editor
            id="quantum.operation.library.mm.quope.presentation.QuantumOpeEditorID"
            name="%_UI_QuantumOpeEditor_label"
            icon="icons/full/obj16/QuantumOpeModelFile.gif"
            extensions="quantumope"
            class="quantum.operation.library.mm.quope.presentation.QuantumOpeEditor"
            contributorClass="quantum.operation.library.mm.quope.presentation.QuantumOpeActionBarContributor">
      </editor>
