<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated quantumOpe -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="quantum.operation.library.mm.quope.presentation.QuantumOpeModelWizardID"
            name="%_UI_QuantumOpeModelWizard_label"
            class="quantum.operation.library.mm.quope.presentation.QuantumOpeModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/QuantumOpeModelFile.gif">
